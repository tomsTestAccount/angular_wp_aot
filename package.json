{
  "name": "angular-examples-master",
  "version": "1.0.0",
  "private": true,
  "description": "Master package.json, the superset of all dependencies for all of the _example package.json files. See _boilerplate/package.json for example npm scripts.",
  "scripts": {
    "rimraf": "rimraf",
    "clean:dist": "rimraf -- dist .awcache",
    "build:webpack:aot": "cd config && npm run clean:dist && webpack --config webpack.config.ts",
    "webpack": "webpack",
    "clean:compile": "rimraf -- \"compiled\" ",
    "ngc:appLogin": "./node_modules/.bin/ngc -p tsconfig.aot.appLogin.json",
    "compile:aot:appLogin": "npm run clean:compile && npm run ngc:appLogin ",
    "ngc:app": "./node_modules/.bin/ngc -p tsconfig.aot.app.json",
    "compile:aot:app": "npm run clean:compile && npm run ngc:app ",
    "clean:dll": "npm run rimraf -- dll",
    "build:dll:appLogin": "cd config && npm run clean:dll && webpack --config webpack.config.ts",
    "build:dll:app": "cd config && npm run clean:dll && webpack --config webpack.config.ts",
    "sass": "node-sass src -o src --include-path node_modules --output-style compressed -q",
    "build:aot:prod:appLogin": "npm run compile:aot:appLogin && npm run build:dll:appLogin && npm run clean:dist && cd config && webpack",
    "build:prod:appLogin": "npm run build:aot:prod:appLogin",
    "build:aot:prod:app": "npm run compile:aot:app && npm run clean:dist && cd config && webpack",
    "build:prod": "npm run build:aot:prod:app",
    "webdev:appLogin": "cd config && webpack-dev-server --config webpack.config.ts",
    "webdev:app": "cd config && webpack-dev-server --config webpack.config.ts",
    "start": "npm run server:dev",
    "server:dev:orig": "npm-run-all -p -r webdev sass:watch",
    "server:dev": "npm run webdev:appLogin"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
