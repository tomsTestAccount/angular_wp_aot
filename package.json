{
  "name": "angular-examples-master",
  "version": "1.0.0",
  "private": true,
  "description": "Master package.json, the superset of all dependencies for all of the _example package.json files. See _boilerplate/package.json for example npm scripts.",
  "scripts": {
    "build": "tsc -p src/",
    "build:watch": "tsc -p src/ -w",
    "build:e2e": "tsc -p e2e/",
    "serve": "lite-server -c=bs-config.json",
    "prestart": "npm run build",
    "start:old": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "protractor": "protractor",
    "webdriver:update": "webdriver-manager update --standalone false --gecko false",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start ./config/karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "build:upgrade": "tsc",
    "serve:upgrade": "http-server",
    "build:cli": "ng build --no-progress",
    "serve:cli": "http-server dist/",
    "build:aot": "ngc -p ./src/tsconfig.aot.json && rollup -c rollup-config.js",
    "serve:aot": "lite-server -c bs-config.aot.json",
    "start:webpack": "webpack-dev-server --inline --progress --port 8080",
    "test:webpack": "karma start karma.webpack.conf.js",
    "test:wp2": "concurrently \"npm run build:watch\" \"karma start karma.webpack.conf.js\"",
    "build:webpack": "rimraf dist && webpack --config config/webpack.prod.js --bail",
    "build:webpack:aot_orig": "rimraf dist && webpack ",
    "build:babel": "babel src -d src --extensions \".es6\" --source-maps",
    "copy-dist-files": "node ./copy-dist-files.js",
    "i18n": "ng-xi18n",
    
	"rimraf": "rimraf",
	"clean:dist": "rimraf -- dist .awcache",
    "build:webpack:aot": "cd config && npm run clean:dist && webpack --config webpack.config.ts",
    "webpack": "webpack",
    "clean:compile": "rimraf -- \"compiled\" ",
    
	"ngc:appLogin": "./node_modules/.bin/ngc -p tsconfig.aot.appLogin.json",
    "compile:aot:appLogin": "npm run clean:compile && npm run ngc:appLogin ",
	
	"ngc:app": "./node_modules/.bin/ngc -p tsconfig.aot.app.json",
    "compile:aot:app": "npm run clean:compile && npm run ngc:app ",
	
	"clean:dll": "npm run rimraf -- dll",
	"build:dll:appLogin": "cd config && npm run clean:dll && webpack --config webpack.config.ts",
	"sass": "node-sass src -o src --include-path node_modules --output-style compressed -q",
	"build:aot:prod": "npm run clean:dist && cd config && webpack",
	
	"webdev:appLogin": "cd config && webpack-dev-server --config webpack.config.ts",
	"webdev:app": "cd config && webpack-dev-server --config webpack.config.ts",
	"start": "npm run server:dev",
	"server:dev:orig": "npm-run-all -p -r webdev sass:watch",
	"server:dev": "npm run webdev"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
